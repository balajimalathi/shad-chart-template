{
  "name": "react-chart-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:all": "npm run build:pieL && npm run build:pieD && npm run build:radi && npm run build:line && npm run build:barI && npm run build:barM && npm run build:area",
    "build:pieL": "vite build --config vite.pieLegend.config.ts",
    "build:pieD": "vite build --config vite.pieDonut.config.ts",
    "build:radi": "vite build --config vite.radicalStacked.config.ts",
    "build:line": "vite build --config vite.lineInteractive.config.ts",
    "build:barI": "vite build --config vite.barInteractive.config.ts",
    "build:barM": "vite build --config vite.barMultiple.config.ts",
    "build:area": "vite build --config vite.areaInteractive.config.ts"
  },
  "dependencies": {
    "@radix-ui/react-select": "^2.2.2",
    "@tailwindcss/vite": "^4.1.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.503.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.3",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/node": "^22.15.3",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "tw-animate-css": "^1.2.8",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1",
    "vite-plugin-singlefile": "^2.2.0"
  }
}